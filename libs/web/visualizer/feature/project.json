{
  "name": "web-visualizer-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/web/visualizer/feature/src",
  "prefix": "as",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/web/visualizer/feature/src/**/*.ts",
          "libs/web/visualizer/feature/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/web/visualizer/feature"],
      "options": {
        "jestConfig": "libs/web/visualizer/feature/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:feature", "scope:web"]
}
